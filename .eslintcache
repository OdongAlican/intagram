ge}\n        />\n        <input type=\"submit\" />\n      </form>\n    </div>\n  );\n}\n\nexport default NewMessageForm;\n",{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","replacedBy":"150"},{"ruleId":"151","replacedBy":"152"},{"ruleId":"153","replacedBy":"154"},{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","severity":1,"message":"158","line":7,"column":3,"nodeType":"159","messageId":"160","endLine":7,"endColumn":14},{"ruleId":"157","severity":1,"message":"158","line":8,"column":3,"nodeType":"159","messageId":"160","endLine":8,"endColumn":14},{"ruleId":"157","severity":1,"message":"158","line":29,"column":9,"nodeType":"159","messageId":"160","endLine":29,"endColumn":20},{"ruleId":"157","severity":1,"message":"158","line":19,"column":5,"nodeType":"159","messageId":"160","endLine":19,"endColumn":16},{"ruleId":"157","severity":1,"message":"158","line":22,"column":5,"nodeType":"159","messageId":"160","endLine":22,"endColumn":16},{"ruleId":"157","severity":1,"message":"158","line":21,"column":5,"nodeType":"159","messageId":"160","endLine":21,"endColumn":16},{"ruleId":"157","severity":1,"message":"158","line":37,"column":7,"nodeType":"159","messageId":"160","endLine":37,"endColumn":18},"lines-around-directive",["161"],"no-spaced-func",["162"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"no-console","Unexpected console statement.","MemberExpression","unexpected","padding-line-between-statements","func-call-spacing"]